var Utilities = function() { };

Utilities.getParams = function(name) {
    name = name.replace( /[\[]/ , "\\\[").replace( /[\]]/ , "\\\]");
    var regexS = "[\\?&]" + name + "=([^&#]*)";
    var regex = new RegExp(regexS);
    var href = decodeURI(window.location.href);
    var results = regex.exec(href);
    if (results == null)
        return "";
    else
        return unescape(results[1]);
};

Utilities.removeParam = function(nameWithValue) { 
    var locationHrefString = window.location.href;
    locationHrefString = locationHrefString.replace(nameWithValue, "");
    return locationHrefString;
};


Utilities.setActiveMenu = function() {
    var url = window.location.href.replace(window.location.protocol + '/', '').replace('/' + window.location.host, '');

    if ($('#sidebar').find("a[href='" + url + "']").length == 0) {
        if (url.indexOf('?') > 2)
            var url = url.substr(0, url.indexOf('?'));

        if ($('#sidebar').find("a[href='" + url + "']").length == 0)
            var url = url.replace('/edit/', '').replace('add', '');

        if (url.length - 1 == url.lastIndexOf('/'))
            url = url.substr(0, url.lastIndexOf('/'));
    }

    if ($('#sidebar').find("a[href='" + url + "']").length > 0) {
        $('#sidebar').find("li").removeClass('active');
        $('#sidebar').find("a[href='" + url + "']").parent().addClass('active');
    }
};

Utilities.setTitle = function() {
    var activeAnchor = $('#sidebar li.active a');
    if (activeAnchor.length)
        $('#sidebar_title').html(activeAnchor.text());
};


Utilities.getActivePageNumberForGrid = function () {
    var pageNumber = location.hash.replace("#", "");
    return pageNumber;
};

Utilities.reloadPage = function () {
    document.location.href = document.location.href;
};

Utilities.isIOSDevice = !!navigator.platform && /iPad|iPhone|iPod/.test(navigator.platform);



;

var VrunVariable = { };
VrunVariable.appUrl = 'http://beatmaker.tv/',
VrunVariable.isAuth = 'False';


function UpdateImage(img, url) {
    if (url) {
        img.attr('src', url);
    } else {
        var t = new Date();

        img.attr('src', img.attr('src') + "&stamp" + t.getMilliseconds());
    }
    //alert(img.attr('src'));
};

function UpdateImageStyle(img, url) {

    img.css('background', "url(" + url + ")");

    
};



function ImgError(source){
    if ($(source).hasClass('small')) {
        source.src = VrunVariable.appUrl + "Content/Images/bf.png";
    }
    else {
        source.src = VrunVariable.appUrl + "Content/Images/bf_big.png";
    }

    if ($(source).data('type') === 'beatmaker-ava')
    {
        source.src = VrunVariable.appUrl + "Content/1CSS/img/default-ava-200.webp";
    }
	if ($(source).data('type') === 'mc-ava') {
        source.src = VrunVariable.appUrl + "Content/1CSS/img/default-ava-200.webp";
	}
    if ($(source).data('type') === "wallpaper")
    {
        source.src = VrunVariable.appUrl + "Content/1CSS/img/default-bg.webp";
    }
    if ($(source).data('type') === "wallpaper-user")
    {
        source.src = VrunVariable.appUrl + "Content/1CSS/img/wallpaper-mc.png";
    }
    
    if ($(source).data('type') === "video") {
        source.src = VrunVariable.appUrl + "Content/1CSS/img/novideo.jpg";
    }
    
    if ($(source).data('type') === "battle") {
        source.src = VrunVariable.appUrl + "content/images/stubs/beatmaker-battle.png";
    }
    if ($(source).data('type') === "battle_small") {
        source.src = VrunVariable.appUrl + "content/images/stubs/beatmaker-battle-small.png";
    }
        
    //source.onerror = null;
    return true;
}

function notImage(source) {
    if ($(source).hasClass('js_img_battle_small')) {
        source.src = VrunVariable.appUrl + "content/images/stubs/beatmaker-battle-small.png";
    }
    if ($(source).hasClass('js_img_battle')) {
        source.src = VrunVariable.appUrl + "content/images/stubs/beatmaker-battle.png";
    }
    return true;
};
     function PutDataToMoreBeatsTooltip(newValue) {
             var data = {
                 beatmakerId: newValue,
                 styleId: 0,
                 moodId: 0,
                 sortExpress: "",
                 samplingOrComposing: "",
                 currentPage: 1
             };

             App.req
                 .newRequest("beats-get-tooltips-beats")
                        .setData(data)
                        .setUrl(App.Vars.urls.getTooltipBeats)
                        .offDefaultSuccess()
                        .addSuccess(OnRequestCompleteGetBeatsForToolTip)
                        .send();
     };

     function OnRequestCompleteGetBeatsForToolTip(result) {
         var beats = result;
     
         $("#toolTipBeatsTemplate").tmpl({ beats: beats }).appendTo(beats_tooltip_container);

         setInterval(yo.favoriteBeats.updateAllElements, 500);
         
     }

     

     $(function () {
         $(document).on("mouseleave", ".moreBeats", function () {
             $(".moreBeats").hide();
         });

         $(document).on('click', ".showTooltip", function () {
             $(".moreBeats").hide();

             if ($(this).find(".moreBeats").is("div.moreBeats")) {
                 $(this).find(".moreBeats").show();
             } else {
                 $(this).append("<div class='moreBeats' id='" + $(this).attr("id") + "'></div>");
                 
                 beats_tooltip_container = $(this).find(".moreBeats").first();

                 /*alert(beats_tooltip_container.attr("id"));*/

                 PutDataToMoreBeatsTooltip($(this).attr("id"));
             }
         });
     });




