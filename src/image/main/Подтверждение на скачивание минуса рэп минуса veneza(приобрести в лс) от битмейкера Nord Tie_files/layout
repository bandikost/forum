        //}, timeOutManager.needUpdateDataParamDefault.updateIfNeedRunOperation);


		App.resp.setCaptchaProvider(App.CaptchaProvider);

	})(App.timeOutManager);
});
$(function() {
	var isShowed = false;
	var touchStartX = 0;
	var touchStartY = 0;
	//todo: recount middle of screen when orient changed
    var middleOfScreen = $(window).width() / 2;
    var startPointForShowingMenu = (Utilities.isIOSDevice ? middleOfScreen : 0) + 10;
    var $menu = $("#js-side-menu");
    var menuWidth = $("#js-side-menu").width();
    var touchStartDate = null;
    var touchEndDate = null;
    var positionChangedByMoving = false;

	//$(document).click(function(event) {
	//	if (!$(event.target).closest('#js-side-menu').length) {
	//		if ($('#js-side-menu').hasClass("active")) {
	//			$("#js-hide-side-menu").trigger("click");
	//		}
	//	}
	//});

	$(document).on("click", "#js-hide-side-menu, #js-show-side-menu", function() {
		toggleMenu();
	});

    function toggleMenu() {
        
        //if (!isShowed) $("#js-show-side-menu").hide();
        //if (isShowed) $("#js-show-side-menu").show();

		isShowed = !isShowed;
        if (isShowed) {
            $menu.css({ left: "0px" });
        }else {
            $menu.css({ left: "" });
        }

        $menu.toggleClass("active");
		
		
	}

    $(document).on("touchstart", function (ev) {
        touchStartDate = new Date();
		var touches = ev.originalEvent.changedTouches;
		touchStartX = touches[0].pageX;
        touchStartY = touches[0].pageY;
        if (touchStartX <= startPointForShowingMenu && !Utilities.isIOSDevice) {
            $(document).on("touchmove.menu-listener", onTouchMove);
            $menu.css({ "transition": "" });
            positionChangedByMoving = true;
        }else {
            positionChangedByMoving = false;
        }
	});


    function onTouchMove(ev) {
        var touches = ev.originalEvent.changedTouches;
        var touchX = touches[0].pageX;
        var touchY = touches[0].pageY;
        if (touchX <= menuWidth){
            $menu.css({ left: (touchX - menuWidth) + "px" });
        }
        //console.log(touchEndX);
    }

    $(document).on("touchend", function (ev) {
        touchEndDate = new Date();

        var touches = ev.originalEvent.changedTouches;

        var startEndDuration = touchEndDate - touchStartDate;
        if (positionChangedByMoving) {
            $menu.css({ left: "" });
            $(document).off("touchmove.menu-listener", onTouchMove);
        }
        //only for android
        if (startEndDuration > 1000 && touchStartX <= startPointForShowingMenu && !Utilities.isIOSDevice) {
           
            var touchEndX = touches[0].pageX;
            $menu.css({ "transition": "" });
            if (!isShowed && touchEndX > (menuWidth / 2 + 20)) {
                toggleMenu();
            }else {
                $menu.css({ left: "" });
            }

         } else {
          
            var touchEndX = touches[0].pageX;
            var touchEndY = touches[0].pageY;

            var yDelta = Math.abs(touchEndY - touchStartY);
            var xDelta = touchEndX - touchStartX;
          
            if (!isShowed && yDelta < 10 && touchStartX <= startPointForShowingMenu && xDelta > 100) {
                toggleMenu();
            }
        }
       
		
	});

	$(document).on("touchstart", "#js-side-menu", function (ev) {
		var touches = ev.originalEvent.changedTouches;
		touchStartX = touches[0].pageX;
	});

	$(document).on("touchend", "#js-side-menu", function(ev) {
		var touches = ev.originalEvent.changedTouches;
		var touchEndX = touches[0].pageX;
		var distanse = (touchEndX - touchStartX);
		if (isShowed && distanse < -20) {
			toggleMenu();
		}
	});
});;
$(function ()
{
	(function (timeOutManager) {

		var lsKEY = "lastServiceUpdatesVisit";

		App.serverUpdatesNotificationManager = {
			setCurrentDate: function () {
				
				timeOutManager.putToday(lsKEY);
			}
		};


		var $serviceUpdatesNotification = $("[data-role='service-updates-notification']");
		
		timeOutManager.doIfNeedIt(1 / 24 /60 *1, lsKEY, function (prevDate) {
			var unixDate = prevDate.getTime() / 1000;
			App.req.newRequest('checkIfHasUpdates')
				.setUrl(App.Vars.urls.serviceUpdatesNeedShow)
				.setData({ prevDate: unixDate })
				.addSuccess(function (result) {
					if (result.Extra.needShow) {
						$serviceUpdatesNotification.show();
						$serviceUpdatesNotification.click(function () {
							App.htmlHelper.showInfoPopup(App.Vars.urls.serviceUpdatesPopup + "?prevDate=" + unixDate);
						});
					}
					else {
						timeOutManager.putToday(lsKEY);
					}
				})
				.send();
		}, 3);

	})(App.timeOutManager);

});
