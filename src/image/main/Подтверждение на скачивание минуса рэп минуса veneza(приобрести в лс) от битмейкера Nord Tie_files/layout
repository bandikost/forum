        }).replace(a.evaluate || h, function (a, b) { return "';" + k(b) + "out+='" }) + "';return out;").replace(/\n/g, "\\n").replace(/\t/g, "\\t").replace(/\r/g, "\\r").replace(/(\s|;|\}|^|\{)out\+='';/g, "$1").replace(/\+''/g, ""); c && (a.selfcontained || !m || m._encodeHTML || (m._encodeHTML = f.encodeHTMLSource(a.doNotSkipEncoded)), b = "var encodeHTML = typeof _encodeHTML !== 'undefined' ? _encodeHTML : (" + f.encodeHTMLSource.toString() +
        "(" + (a.doNotSkipEncoded || "") + "));" + b); try { return new Function(a.varname, b) } catch (q) { throw "undefined" !== typeof console && console.log("Could not create a template function: " + b), q; }
    }; f.compile = function (b, a) { return f.template(b, null, a) }
})();;
$(function() {
	(function(timeOutManager) {
		$("#Info-block .close").click(function() {
			$("#Info-block").fadeOut(500);
		});

	
		$("#js-search-beatmaker").autocomplete({
			source: App.Vars.urls.findBeatmaker,
			minLength: 1,
			autoFocus: true,
			select: function(event, ui) {
				
				App.htmlHelper.clickChangeUrlHandler(App.Vars.urls.beatmakerPage + ui.item.id);
			}
		});


		$(document).on("click", ".js-info-popup", function() {
			App.htmlHelper.showInfoPopup(App.Vars.urls.infoPopup + $(this).data("info-popup-model"));
		});
		

		var offset = $(window).height() / 2;

		if ($(this).scrollTop() > offset) {
			$(".to-top").fadeIn();
		} else {
			$(".to-top").fadeOut();
		}


		if ($(window).width() <= 1200) {
			$(".to-top").addClass("minimized");
		} else {
			$(".to-top").removeClass("minimized");
		}

		var lastScrollTop = 0;


		var headerTimeout = null;

		globalScrollHeaderFix = function() {
			var st = $(window).scrollTop();

			if ($(window).width() <= 1200) {
				$(".to-top").addClass("minimized");

			} else {
				$(".to-top").removeClass("minimized");

			}

			if (st > offset) {
				$(".to-top").fadeIn();

			} else {
				$(".to-top").fadeOut();

			}

			clearTimeout(headerTimeout);


			lastScrollTop = st;
		}

		$(window).scroll(function() {
			globalScrollHeaderFix();
		});


		yo.favoriteBeats.updateAllElements();


		//  todo:move to page
		$("#product-images a").fancybox();

		//ymaps.ready(
		//	GeoLocationProvider.initMyCityStudios);

		$('.to-top').click(function(event) {
			event.preventDefault();
			$('html, body').animate({
				scrollTop: $("#header").offset().top
			}, 500);
			return false;
		});

		showIfNeedShareBeatPopup();

		function showIfNeedShareBeatPopup() {

			var lastBeatSharing = App.LS.get("lastBeatSharing");

			var d = new Date();
			var limitDate = new Date();
			limitDate.setDate(d.getDate() - 3);

			if (!lastBeatSharing) {

				d.setDate(d.getDate() - 10);
				lastBeatSharing = { lastDate: d }
			} else {
				lastBeatSharing.lastDate = new Date(lastBeatSharing.lastDate);
			}

			if (lastBeatSharing.lastDate < limitDate) {
				lastBeatSharing.lastDate = new Date();
				App.LS.put("lastBeatSharing", lastBeatSharing);

				App.req.newRequest("getBeatForShare")
					.setUrl(App.Vars.urls.getBeatForShare)
					.addSuccess(function(result) {
						if (result.Extra.NeedShare) {
							App.htmlHelper.showInfoPopup(App.Vars.urls.infoPopup + "?model=ShareBeat&beatId=" + result.Extra.BeatId);
						};
					})
					.send();

			}
		}

		var banner = $("#ads-banner");
		var url = banner.attr('href');

		function openBannerUrl() {
			window.open(url);
			$(document).off("click", "a", openBannerUrl);
			return true;
		}

		if (banner.hasClass("js-active")) {
			clickIfNeedOnBannerAds();
		}

		function clickIfNeedOnBannerAds() {
			timeOutManager.doIfNeed(2, "lastBannerClick", function() {
				$(document).on("click", "a", openBannerUrl);
			}, true);
		}

		function checkJudge() {
			if (App.Vars.isAuth == 'False')
				return;

			App.req.newRequest("checkJudge")
				.setUrl(App.Vars.urls.checkJudge)
				.addSuccess(function(result) {
					if (result.Extra.IsJudge == false)
						return;

					if (!result.Extra.TrackId)
						return;

					App.htmlHelper.showInfoPopup(App.Vars.urls.rateTrack + "?trackId=" + result.Extra.TrackId);
				})
				.send();
		}

		
		timeOutManager.doIfNeed(0.01, "checkJudge", function () {
			checkJudge();
		}, true);

		//todo: what is it
		$(document).on("click", ".js-premium-acc", function() {
			/*$(this).attr("title", "");*/
			App.htmlHelper.showInfoPopup(App.Vars.urls.premiumBeatsInfo);
		});

		$(document).on("click", "#js-hide-info-widget", function() {
			/*$(this).attr("title", "");*/
            $(".js-info-widget").removeClass("full");
			//timeOutManager.putToday("lastInfoWidgetClosed");
        });

        $(".js-info-widget").on("click", ".overlay", function () {
            $(".js-info-widget").addClass("full");
            //timeOutManager.putToday("lastInfoWidgetClosed");
        });

       
        $(".js-menu-user").on("click", "ul li.dropdown-submenu", function (e) {
           
        });

        var ignoreSubmenu = false;


        function moveSubmenu($li, e) {
           
            e.stopPropagation();

            var $submenu = $li.find('ul');
            var $menu = $li.width();
            var submenuW = $submenu.width();
            var windowW = $(window).width();

            var summW = $menu + submenuW;

            var newLftValue = 0;
            if (summW > windowW) {
                newLftValue = 0;
                $submenu.css({ "left": newLftValue + "px" });
            }else {
                $submenu.css({ "left": null });
            }
        }

       

        function hideSubmenuForTouchScreens() {
            setTimeout(function () {
                $(".js-menu-user").find(".dropdown-submenu ul").css({ left: '999999px' });

            }, 1000);
        }


        $(".js-menu-user").on("touchstart", "ul li a", function (e) {

            var $li = $(this).closest("li");

            if ($li.hasClass("dropdown-submenu")) {
               
            } else {

                hideSubmenuForTouchScreens();
            }
        });

       
        $(".js-menu-user").on("click", "ul li a", function (e) {
            
            var $li = $(this).closest("li");

            if ($li.hasClass("dropdown-submenu")) {
                moveSubmenu($li, e);
                return false;
            }
            
           // ignoreSubmenu = true;
            $(".js-menu-user").find(".dropdown").removeClass("open")
                .find(".dropdown-toggle").attr("aria-expanded", false);

//            if (e.type === "touchstart") {
               
  //          }
            //e.stopPropagation();
            //return true;
            
        });
        //timeOutManager.doIfNeedIt(0.00069, "special-beatmaker-popup", function () {
        //    axios.get(App.Vars.urls.fullScreenPopupSpecialBeatmakerId)
        //        .then((result) => {
