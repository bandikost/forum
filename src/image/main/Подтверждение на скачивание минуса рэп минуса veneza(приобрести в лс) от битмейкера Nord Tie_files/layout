$.validator.methods.number = function (value, element) {
	var optional = this.optional( element );
	var result = optional || /^-?(?:\d+|\d{1,3}(?:,\d{3})+)?(?:[\.,]\d+)?$/.test(value);
	return result;

};

//original
//number: function( value, element ) {
//	return this.optional( element ) || /^-?(?:\d+|\d{1,3}(?:,\d{3})+)?(?:\.\d+)?$/.test( value );
//},;
;
function FollowMe($btn, userId, settings) {
    this._url = App.Vars.urls.followMe + '?userId=' + userId;
    this._$btn = $btn;
    this.settings = settings;
    //.isSubscribed;
    
    settings.autoUnsubscribeText = !settings.unsubscribeText;
    settings.autoSubscribeText = !settings.subscribeText;
    //settings.subscribeText;
    //settings.unsubscribeDisabled = $btn.data("unsubscribe-disable");
   
    var that = this;

    $btn.click(function () {
        if (App.isAnonym) {
            alert('');
            return;
        }    

        if (!!settings.unsubscribeDisabled && settings.isSubscribed) {
            return;
        }

        settings.isSubscribed = !settings.isSubscribed;
        
        App.req.newRequest("switch-starred")
            .setUrl(that._url)
            .addSuccess(function (data) {
                that.settings.isSubscribed = (data.Extra["isActive"] == true);
                that.settings.followersCount = data.Extra["count"];
                that._prepareHtml();
            })
            .send();
    });

    this._prepareHtml = function() {
        
        if (this.settings.autoUnsubscribeText) {
            this.settings.unsubscribeText = "" + " (" + this.settings.followersCount + ")";
        }
        if (this.settings.autoSubscribeText) {
            this.settings.subscribeText = "" + " (" + this.settings.followersCount + ")";
        }

        if (this.settings.isSubscribed) {
            this._$btn.html(this.settings.unsubscribeText);
        } else {
            this._$btn.html(this.settings.subscribeText);
        }
    }

    this._prepareHtml();
};;


//  
function ContextMenu($wrapper) {
    this._$wrapper = $wrapper;
    this._$switchButton = this._$wrapper.byRole("show-menu");
    // find the menu inside the button
    this._$menu = this._$wrapper.find('div[data-role="menu"]');
    this._justShowed = false;
}

// add the methods to the prototype
ContextMenu.prototype = {
    // on click of the button we either hide or show the menu
    _onSwitchButtonClick: function (e) {
        if (this._$menu.hasClass('hidden')) { // if the menu is hidden, we show it
            this._showMenu(e);
        } else {
            this._hideMenu();
        }
    },

    _showMenu: function (e) {
        this._$menu.removeClass('hidden');
        // to check whether or not the button is too close to the top of the window we create 3 var's
        var menuButtonDistanceStr = this._$menu.css("bottom");
        var buttonOffset = this._$switchButton[0].getBoundingClientRect().top;
        var menuButtonDistance = parseInt(menuButtonDistanceStr, 10);
        var menuHeight = this._$menu.height() + menuButtonDistance;

        // if the button is too far up the window, we show the menu on its bottom side
        if (buttonOffset < menuHeight) {
            this._$menu.addClass('go-down');
            this._$menu.css('top', -menuHeight);
        }
        this._justShowed = true;
        $('body').on('click.widgetShowMenu', this._onBodyClick.bind(this));
        
    },

    // we hide the menu
     _hideMenu: function () {
       
        this._$menu.addClass('hidden');
        $('body').off('click.widgetShowMenu');
    },

     _onBodyClick: function (e) {
       
        if (this._justShowed == false) {
            this._hideMenu();
        } else {
            this._justShowed = false;
        }
    },


    // initialize, add a listener to the button
    init: function () {
        this._$switchButton.on('click', this._onSwitchButtonClick.bind(this));

        this._$menu.find(".item").on('click', this._hideMenu.bind(this));

    }
};
class HtmlUrlResolver {
    constructor() {
    }
    parseLinks(text) {
        var expression = /https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}\b([-a-zA-Z0-9@:%_\+.~#?&//=]*)?/gi;
        var resultString = text.replace(expression, "<a href='$&' target='_blank'>$&</a>");
        return resultString;
    }
}
//# sourceMappingURL=html-url-resolver.js.map;
(function () {
	var BrowserIdCreator = function() {
		
	}
	BrowserIdCreator.prototype = {
		putHiddenField: function ($formEl) {
			var $hiddenInput = $("<input>")
				.attr("type", "hidden")
				.attr("name", "cID")
				.val(App.LS2.load("cID"));
			$formEl.append($hiddenInput);
		}
	}

	App.idCreator = new BrowserIdCreator();

	function uuidv4() {
		return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
			var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
			return v.toString(16);
		});
	}

    var currentCid = App.LS2.load("cID", null);
    // put cID if not created
    if (!currentCid) {
        currentCid = uuidv4();
        App.LS2.save("cID", currentCid);
        console.log('cID created');

    }

   
    App.req.prototype.customHeaders = { "btv-cid": currentCid }
})();;
