class BaseAPIService {
    makeRequest(type, url, data) {
        return $.ajax({
            type: type,
            url: url,
            data: data
        });
    }
}
//# sourceMappingURL=BaseAPIService.js.map;
var __awaiter = (this && this.__awaiter) || function (thisArg, _arguments, P, generator) {
    function adopt(value) { return value instanceof P ? value : new P(function (resolve) { resolve(value); }); }
    return new (P || (P = Promise))(function (resolve, reject) {
        function fulfilled(value) { try { step(generator.next(value)); } catch (e) { reject(e); } }
        function rejected(value) { try { step(generator["throw"](value)); } catch (e) { reject(e); } }
        function step(result) { result.done ? resolve(result.value) : adopt(result.value).then(fulfilled, rejected); }
        step((generator = generator.apply(thisArg, _arguments || [])).next());
    });
};
class BasePopup {
    constructor(clickBodyCallback) {
        this.showed = false;
        this.bannerTemplate = '';
        this._$el = null;
        this._$elHeader = null;
        this._$elBody = null;
        this._$elBg = null;
        this._$elClose = null;
        // in seconds
        this._closeButtonShowDelay = 0;
        BasePopup.globalIndex++;
        this.index = BasePopup.globalIndex;
        this.bannerTemplate = `
<div class='ads-popup-background' data-role='vroon-popup-bg-${this.index}'></div>
<div data-role='vroon-popup-${this.index}' id='popup' class='ads-popup '>
    <button data-role='close' class='close-button display-none'>
        <span class='glyphicon glyphicon-remove-circle'></span>
    </button>
    <div data-role='ads-popup-header' class='header popup-title'>
          !    
    </div>
    <div data-role='ads-popup-body' class='content'></div>
</div>


`;
        $('body').append($(this.bannerTemplate));
        this._$el = $.byRole("vroon-popup-" + this.index);
        this._$elHeader = this._$el.byRole("ads-popup-header");
        this._$elBody = this._$el.byRole("ads-popup-body");
        this._$elBg = $.byRole("vroon-popup-bg-" + this.index);
        this._$elClose = this._$el.byRole("close");
        this._$elClose.click(this._onCloseClick.bind(this));
        this._$elBody.click(this._onBodyClick.bind(this, clickBodyCallback));
    }
    beforeRender() {
        return Promise.resolve();
    }
    show() {
        return __awaiter(this, void 0, void 0, function* () {
            yield this.beforeRender();
            this._$elBg.addClass("open");
            this._$el.addClass("open");
            $('body').addClass("ads-popup-showed");
            this.showed = true;
            this._onResize();
            console.log("_closeButtonShowDelay: " + this._closeButtonShowDelay);
            setTimeout(() => {
                this._$elClose.removeClass("display-none");
            }, this._closeButtonShowDelay * 1000);
            $(window).on("scroll.adspopup resize.adspopup", this._onResize.bind(this));
        });
    }
    _scrollToTheTop() {
        window.scrollTo(0, 0);
    }
    _onResize() {
        this._scrollToTheTop();
        this._setCorrectSize();
    }
    hide() {
        this._$elBg.removeClass("open");
        this._$el.removeClass("open");
        $('body').removeClass("ads-popup-showed");
        this.showed = false;
        this._$elBody.html('');
        this._$elHeader.html('');
        $(window).off("scroll.adspopup resize.adspopup");
    }
    putContent(html, title) {
        this._$elBody.html(html);
        this._$elHeader.html(title);
    }
    _setCorrectSize() {
        const windowWidth = $(window).width();
        const windowHeight = $(window).height();
        const goodWidth = 500;
        if (windowWidth > goodWidth) {
            this._$el.css({
                width: goodWidth
            });
        }
        else {
            this._$el.css({
                width: (windowWidth - 40) + "px"
            });
        }
        var popupHeight = windowHeight - 40;
        var headerHeight = this._$elHeader.height();
        this._$el.css({
            "max-height": popupHeight + "px"
        });
        this._$elBody.css({
            "max-height": (popupHeight - headerHeight) + "px"
        });
    }
    _onCloseClick() {
        this.hide();
    }
    ;
    _onBodyClick(clickBodyCallback) {
        clickBodyCallback();
    }
    ;
}
BasePopup.globalIndex = 0;
