(function() {
    VrunTabs = {

        refresh: function () {
            
            $(function () {

                $(".vr-tabs-activate-tab.active").each(function () {
                    clickMethod($(this), false);
                });
            });

            
            var currentState = history.state;
            
            if (currentState && currentState.tabWiget) {

                _.each(currentState.tabWiget, function(item) {
                    var tabWigetId = item.tabWigetId;
                    var tabId = item.tabId;
                    var el = getTitleFor(tabWigetId, tabId);
                    clickMethod(el, false);
                });
            }
        }
    };
    
    function hideAllTabsForWidget(tabWigetId) {
        $(".vr-tabs-tab[data-vr-tabs-id=" + tabWigetId + "]").hide();
    }
    function showTargetTab(tabWigetId, tabId, $el) {
        var targetTab = $(".vr-tabs-tab[data-vr-tabs-tab-id=" + tabId + "]"
            + "[data-vr-tabs-id=" + tabWigetId + "]");

        if (tabId == "ajax" || $el.hasClass("vr-ajax-tab")) {

        	targetTab = $(".vr-tabs-tab[data-vr-tabs-tab-id=ajax]"
            + "[data-vr-tabs-id=" + tabWigetId + "]");

        	App.req.newRequest("upload-widget-content")
                .setUrl($el.data("href"))
                .putResponseTo(targetTab)
                .send();
        }
        targetTab.show();
    }

    function getTitleFor(tabWigetId, tabId) {
        var targetTitle = $(".vr-tabs-activate-tab[data-vr-tabs-tab-id=" + tabId + "]"
            + "[data-vr-tabs-id=" + tabWigetId + "]");
        return targetTitle;
    }

    function deleteActiveClassFromAllTabsTitles(tabWigetId) {
        $(".vr-tabs-activate-tab[data-vr-tabs-id=" + tabWigetId + "]").removeClass("active");
    }
    function addActiveClassToTargetTabTitle($el) {
        $el.addClass("active");
    }

    function clickMethod($el, replaceHistory) {

        var tabWigetId = $el.data("vr-tabs-id");

        var tabId = $el.data("vr-tabs-tab-id");


        deleteActiveClassFromAllTabsTitles(tabWigetId);
        addActiveClassToTargetTabTitle($el);

        hideAllTabsForWidget(tabWigetId);
        showTargetTab(tabWigetId, tabId, $el);


        if (replaceHistory) {
            var currentState = history.state;

	        if (!currentState)
		        currentState = {};

	        if (!currentState.tabWiget) {
                currentState.tabWiget = [];
            }
            var widget = _.find(currentState.tabWiget, function (item) {
                return item.tabWigetId == tabWigetId;
            });

            if (widget) {
                widget.tabId = tabId;
            } else {
                currentState.tabWiget.push({
                    tabWigetId: tabWigetId,
                    tabId: tabId
                });
            }



            history.replaceState(currentState, currentState.title, currentState.url);
        }
    }

    $(function () {

        $(".js-tabs-widget .js-tabs .js-tab").hide();
        $(".js-tabs-widget .js-tabs").each(function () {
            $(this).find(".js-tab").first().show();
        });
        $(document).on("click", ".js-tabs-widget .js-tabs-titles .js-title", function () {
            
            var widget = $(this).closest(".js-tabs-widget");
            widget.find(".js-tabs-titles .js-title").removeClass("active");
            $(this).addClass("active");

            if (widget.hasClass("js-tabs-widget-rewritten-on-page")) {
                return;

            }

            if (widget.hasClass("js-tabs-widget-ajax")) {

                var targetEl = widget.find(".js-ajax-content").first();
                if ($(this).hasClass("js-rewriteMe")) {
                    targetEl = widget.closest(".js-ajax-content").first();
                }

                /*targetEl.html("...");*/

                App.req.newRequest("load-wiget-content")
                    .setUrl($(this).data("tab-wiget-url"))
                    .offDefaultSuccess()
                    .putResponseTo(targetEl)
                    .send();

            } else {
                widget.find(".js-tabs .js-tab").hide();
                widget.find(".js-tabs #" + this.id + ".js-tab").show();
            }
        });
        

        $(document).on("click", ".vr-tabs-activate-tab", function () {
            clickMethod($(this), true);
        });

    });

})();



