App.htmlHelper = (function() {
	var url = App.LS.get("mustBeRedirectedTo");
	var mustBeRedirected = App.LS.get("mustBeRedirectedToAfterLogin");
	if (!!url && !!mustBeRedirected && App.Vars.isAuth) {
		App.LS.put("mustBeRedirectedTo", null);
		App.LS.put("mustBeRedirectedToAfterLogin", null);
		document.location.href = url;
	}

    $(document).on("click", "[data-role='want-buy-beat']", function (e) {
       // e.preventDefault();
       // e.stopPropagation();

       
            window.ngInterop.publishToClass('ShowSelectLicensePopupEvent', { beatId: +$(this).data("beat-id"), licenseId: $(this).data("license-id") });
        //}
            //App.pages.shoppingCart.cartService.wantAddBeat($(this).data("beat-id"), $(this).data("license-id"));
    });

    let intervalForCart = setInterval(() => {

            if (App.pages.shoppingCart && App.pages.shoppingCart.cartService) {
                App.pages.shoppingCart.cartService.addSubscriber("beat-added",
                    ({ beatId, licenseId }) => {
