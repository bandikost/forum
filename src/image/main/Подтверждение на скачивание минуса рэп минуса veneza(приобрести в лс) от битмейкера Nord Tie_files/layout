var VrunVariable = { };
VrunVariable.appUrl = 'http://beatmaker.tv/',
VrunVariable.isAuth = 'False';
;
App.Animation = function($) {
	
	return {
		hideLoading: function () {
			var $element = $(".global-loading");
            //$.fancybox.hideLoading();
	        $element.removeClass("show");
        },
        showLoading: function () {
	        var $element = $(".global-loading");
        	//$.fancybox.showLoading();
        	//$("#ajaxLoader").show();
        	$element.addClass("show");
        }
    };
}($);;
VrunResponse = function ($) {

    var self = this;
    self.captchaProvider = null;

    self.clearFieldErrors = function(field) {
        $("[data-valmsg-for=" + field + "]")
            .addClass("field-validation-valid")
            .removeClass("field-validation-error")
            .html('');

        $(".js-main-validation-block").removeClass("active");
    }

    self.setCaptchaProvider = function(captchaProvider) {
        self.captchaProvider = captchaProvider;
    }

    self.putResponseToElement = function (response, addition) {
        console.log('before put response to');
        addition[this.constants.putResponseToElementAdditionKeyName].html(response);
    }

    self.putResponseTo = self.putResponseToElement;

    self.appendResponseToElement = function(response, addition) {
        addition[this.constants.putResponseToElementAdditionKeyName].append(response);
    }

    self.redirectTo = function(response, addition) {
        location.href = addition[this.constants.redirectToPageAdditionKeyName];
    }

    self.reloadPage = function() {
        location.href = location.href;
    }

    self.closePopup = function() {
        $.fancybox.close();
    }

    var errorsMessages = [];

    self.showErrorInBlock = function(message, errorBlockId, field) {

        if (field) {
            field = field.replace(".", "\\.");

            $("[data-valmsg-for=" + field + "]")
                .removeClass("field-validation-valid")
                .addClass("field-validation-error")
                .html('<span class="" generated="true">' + message + '</span>');
        } else {
            $(".validation-summary-valid")
                .removeClass("validation-summary-valid")
                .addClass("validation-summary-errors")
                .find("ul")
                .html('');

            $(".validation-summary-errors").hide();

            if (errorBlockId !== "") {
                errorBlockId = "#" + errorBlockId;
            }

            $(errorBlockId + ".validation-summary-errors").show();
            $(errorBlockId + ".validation-summary-errors ul").show();

            $(errorBlockId + ".validation-summary-errors ul").append('<li>' + message + '</li>');

            $(".js-main-validation-block").addClass("active");

            //hack
            $("ul.validation-summary-errors").removeClass("validation-summary-errors");

            $(".ajaxResponseErrorsBlock").append("- " + message + '<br />');
        }

        self.errorClearTimeout = setTimeout(() => self.clearErrors(), 10000);
    }

    self.errorClearTimeout = -100;

    self.clearErrors = function () {
        $(".ErrorFieldBlockInfo").remove();
        $(".ajaxResponseErrorsBlock").html('').hide();
        $(".validation-summary-errors ul").html('');
        $(".validation-summary-errors").addClass("validation-summary-valid").removeClass("validation-summary-errors");
    }

    self.parseResponse = function(response, addition) {
        if (addition === undefined) {
            addition = {
                request: { ok: true }
            };
        }

        self.clearErrors();
        clearTimeout(self.errorClearTimeout);


        if (response.OK === false) {
            for (var i = 0; i < response.Errors.length; i++) {
                if (response.Errors[i].ErrorMessageType === ReponseHandler.ErrorMessageType.LightFields) {
                    $("[name='" + response.Errors[i].Field + "']").after("<span style='color: red; display: block;' class='ErrorFieldBlockInfo'>" + response.Errors[i].Message + "</span>");
                }
                if (response.Errors[i].ErrorMessageType === ReponseHandler.ErrorMessageType.JavaScript) {
                    eval(response.Errors[i].JavaScript);
                }
                if (response.Errors[i].ErrorMessageType === ReponseHandler.ErrorMessageType.ErrorBlock) {
                    this.showErrorInBlock(response.Errors[i].Message);
                }
                if (response.Errors[i].ErrorMessageType === ReponseHandler.ErrorMessageType.Alert) {
                    alert(response.Errors[i].Message);
                }
            }
            addition.request.ok = parseNewResponseBlock(response, addition);
            return addition.request.ok;
        } else {
            $(".ErrorFieldBlockInfo").remove();

            if (response.NeedClosePopup)
                $.fancybox.close();

            if (response.ShowMessageInOKBlock) {
                $(".ajaxResponseOKBlock").append(response.InfoMessage + '<br />');
                setTimeout(function() { $(".ajaxResponseOKBlock").html(""); }, 2000);
            }

            if (response.DoSuccessJS) {
                eval(response.SuccessJS);
            }

            if (response.ShowMessageByAlert) {
                alert(response.InfoMessage);
            }

            if (response.RedirectUrl)
                location.href = response.RedirectUrl;

            if (response.NeedReloadPage)
                location.href = location.href;

            addition.request.ok = this.parseNewResponseBlock(response, addition);
            return addition.request.ok;
        }
    }

    this.parseNewResponseBlock = function (response, addition) {

        function showSuccess(message) {
            $(".js_successMessage").html(message).fadeIn(1000);


            $("#global-success-block").html(message).addClass("active");
            $("#global-error-block").removeClass("active");
        }

        function showSuccessByAlert(message) {
            alert(message);
        }

        var errorBlockId = "";
        if (addition[this.constants.errorsBlock]) {
            errorBlockId = addition[this.constants.errorsBlock];
        }

        var responseBlock = response.Response;
        var isCorrect = true;
        for (var i = 0; i < responseBlock.length; i++) {
            if (responseBlock[i].k === NewReponseBlockKeys.NeedReloadCaptcha) {
                if (!this.captchaProvider) {
                    console.log('You should set CaptchaProvider');
                }
                this.captchaProvider.GetCaptcha();
            }

            if (responseBlock[i].k === NewReponseBlockKeys.Errors) {
                for (var j = 0; j < responseBlock[i].v.length; j++) {

                    errorsMessages.push(responseBlock[i].v[j].message);

                    $(".js_successMessage").hide();

                    if (responseBlock[i].v[j].errorCode) {
                        if (addition[this.constants.customErrors]) {
                            for (n in addition[this.constants.customErrors]) {
                                if (addition[this.constants.customErrors][n].errorCode == responseBlock[i].v[j].errorCode) {
                                    //alert('dfdf');
                                    addition[this.constants.customErrors][n].callback(response, responseBlock[i].v[j].message);
                                }
                            }
                        }
                    } else {

                        this.showErrorInBlock(responseBlock[i].v[j].message, errorBlockId, responseBlock[i].v[j].field);
                    }
                    isCorrect = false;
                }
            }

            if (isCorrect == false && addition.errorsHandler)
                addition.errorsHandler(errorsMessages);

            if (responseBlock[i].k === NewReponseBlockKeys.SuccessMessage) {
            	
            	if (addition[this.constants.showSuccessInAlert]) {
                    showSuccessByAlert(responseBlock[i].v);
                } else {
                    showSuccess(responseBlock[i].v);
                }
            }
            if (responseBlock[i].k === NewReponseBlockKeys.ClosePopup) {
                $.fancybox.close();
            }
            if (responseBlock[i].k === NewReponseBlockKeys.ReloadPage) {
                location.href = location.href;
            }
            if (responseBlock[i].k === NewReponseBlockKeys.RedirectTo) {
                location.href = responseBlock[i].v;
            }
        };
        return isCorrect;
    }

    var NewReponseBlockKeys = {
        OK: "1",
        NeedReloadCaptcha: "2",
        Errors: "3",
        SuccessMessage: "4",
        ClosePopup: "5",
        ReloadPage: "6",
        RedirectTo: "7"
    };

    var ReponseHandler = {
        ErrorMessageType: {
            ErrorBlock: 1,
            LightFields: 2,
            Alert: 3,
            JavaScript: 4
        }
    };

    this.redirectToPage = function(url) {
        location.href = url;
    };

/* return {
        parseResponse: parseResponse,
        setCaptchaProvider: setCaptchaProvider,
        redirectTo: redirectTo,
        redirectToPage: function (url) {
            location.href = url;
        },
        putResponseTo: putResponseToElement,
        appendResponseToElement: appendResponseToElement,
        constants: this.constants,
        closePopup: closePopup,
        reloadPage: reloadPage,
        showErrorInBlock: showErrors,
        clearFieldErrors: clearFieldErrors
    };*/
};

VrunResponse.prototype.constants = {
    putResponseToElementAdditionKeyName: "putResponseToElement",
    redirectToPageAdditionKeyName: "redirectToPage",
    customErrors: "customErrors",
    showSuccessInAlert: "showSuccessInAlert",
    errorsBlock: "errorsBlock",
    errorsHandler: "errorsHandler"
};

  
  App.resp = new VrunResponse($);;
App.req = function (animation, $, ParseUtil) {
    var Request = function () {
        this.requestMethod = "POST";
        this.id = "";
        this.url = "";
        this.data = {};
        this.targetId = "";
        this.targetElement = null;
        this.parentElement = null;
        this.destinationElement = null;
        this.showProccess = true;
        this.successes = [];
        this.errorHandlers = [];
        this.isJsonContentType = false;
       
        this.defaultMethod = true;
        this.ok = true;

        //add addition value for response callbacks
        this.addition = {};

        var request = this;

        /*this.success = [function (data) {
            //  alert(request.targetId);
            if (request.targetElement != null) {
                request.targetElement.html(data);
            } else if (request.targetId) {
                //alert($("#" + request.targetId).html());
                $("#" + request.targetId).html(data);
                //$("#" + request.targetId).css('opacity', '1');
            } else {
                request.ok = ParseUtil.parseResponse(data);

                return request.ok;
            }
            return true;
        }];*/
    };

    Request.newRequest = function (requestId) {
        var request = new Request();

        request.setRequestId(requestId);
        
        return request;
    };

    Request.new = function (requestId) {
        var request = new Request();

        request.axios = true;
        request.setRequestId(requestId);

        return request;
    };

    /*Request.prototype.newRequest = function (requestId) {
        var request = new Request();
        if (requestId) {
            request.setRequestId(requestId);
        }
        return request;
    };*/

    Request.DefaultSuccessMethod = function (result, addition) {
        App.resp.parseResponse(result, addition);
    };
    
    Request.ResponceCallbacks = [];

    Request.RequestsSuccesses = [];
    Request.AdditionsForRequest = [];
    Request.RequestsPermanentSuccesses = [];
    Request.Bindings = [];

    Request.prototype.setParentElement = function (element) {
        this.parentElement = element;
        return this;
    };

    Request.prototype.setJsonContentType = function () {
        this.isJsonContentType = true;
        return this;
    };
    
    
    Request.prototype.addAddition = function (key, value, object) {
        if (this.addition[key]) {
            if (object && object.putToArray) {
                this.addition[key].push(value);
            } else {
                //this.addition[key];
            }
        } else {
            if (object && object.putToArray) {
                this.addition[key] = [];
                this.addition[key].push(value);
            } else {
                this.addition[key] = value;
            }
        }
        
        return this;
    };

    Request.prototype.addSuccess = function (method) {
        var self = this;
        
        self.successes.push({ id: this.id, function: method });


       /* if (!_.some(Request.RequestsSuccesses, function(item) {
            return item.id == self.id;
        })) {
            Request.RequestsSuccesses.push({ id: this.id, function: method });
        }*/
        return this;
    };

    Request.prototype.addError = function (method) {
    	var self = this;

    	self.errorHandlers.push({ id: this.id, function: method });


    	/* if (!_.some(Request.RequestsSuccesses, function(item) {
			 return item.id == self.id;
		 })) {
			 Request.RequestsSuccesses.push({ id: this.id, function: method });
		 }*/
    	return this;
    };
    
    Request.prototype.addResponceCallback = function (method) {
        Request.ResponceCallbacks.push({ id: this.id, function: method });
        return this;
    };
    
    Request.bindSuccessWithRequestId = function (requestId, callback, bindingName) {
         /* if (!_.some(Request.RequestsSuccesses, function(item) {
            return item.id == self.id;
        })) {
            Request.RequestsSuccesses.push({ id: this.id, function: method });
        }*/


        if (bindingName) {
            for (i in Request.Bindings) {
                if (Request.Bindings[i] === bindingName) {
                    return;
                }
            }
            Request.Bindings.push(bindingName);
        }
        Request.RequestsPermanentSuccesses.push({ id: requestId, function: callback });
    };

    Request.bindOnceSuccessWithRequestId = function(requestId, callback) {
        Request.RequestsSuccesses.push({ id: requestId, function: callback });
    };
    
    Request.bindAdditionWithRequestId = function (requestId, key, value, object) {
        var valueToPutting = null;

        function findAddition() {
            for (i in Request.AdditionsForRequest) {
                if (Request.AdditionsForRequest[i].id == requestId && Request.AdditionsForRequest[i].addition.key == key) {
                    valueToPutting = Request.AdditionsForRequest[i].addition;
                }
            }
        }

        findAddition();

        if (valueToPutting) {
            if (object && object.putToArray) {
                valueToPutting.value.push(value);
            } else {
                //Request.AdditionsForRequest[key];
            }
        } else {
            Request.AdditionsForRequest.push({ id: requestId, addition: { key: key } });
            findAddition();
            if (object && object.putToArray) {
                valueToPutting.value = [];
                valueToPutting.value.push(value);
            } else {
                valueToPutting.value = value;
            }
        }
        
        //
    };

    Request.prototype.customHeaders = null;

    Request.prototype.offDefaultSuccess = function () {
        this.defaultMethod = false;
        return this;
    };
    
    Request.prototype.offUploadAnimation = function () {
        this.showProccess = false;
        return this;
    };

    Request.prototype.setMethod = function (method) {
        this.requestMethod = method;
        return this;
    };

    Request.prototype.setGET = function () {
        this.requestMethod = "GET";
        return this;
    };

    Request.prototype.setUrl = function (url) {
        this.url = url;
        return this;
    };
    Request.prototype.setRequestId = function (requestId) {
        if (requestId == undefined)
            throw Error("We must determine ID");
        this.id = requestId;
        return this;
    };

    Request.prototype.setTargetId = function (targetId) {
        this.targetId = targetId;
        return this;
    };

    Request.prototype.setTargetElement = function (targetEl) {
        this.targetElement = targetEl;
        return this;
    };

    Request.prototype.setData = function (data) {
        this.data = data;
        return this;
    };

    Request.prototype.post = function (url) {
        this.setMethod("POST");
        this.setUrl(url);
        return this;
    };

    Request.prototype._sendAxios = function () {
        return axios({
            method: this.requestMethod,
            url: this.url,
            data: this.data
        }).then(data => data.data)
            .then(result => {
                successCallback(result, this);
                return result;
            })
            .catch((error) => {
                animation.hideLoading();
                $("#serverError").fadeIn(500);
                return error;
            });
    }

    Request.prototype.send = function () {
        if (this.targetId) {
            //$("#" + this.targetId).css('opacity', '.5');
        }

        var request = this;
        hideServerErrorBlock();

        if (request.showProccess)
            animation.showLoading();


        var ajaxSettings = {
            type: this.requestMethod,
            url: this.url,
            data: this.data,

            /*contentType: "application/json; charset=utf-8",*/
            /* dataType: "json",*/
            success: function (result) {
                successCallback(result, request);
            },
            error: function (error) {
                animation.hideLoading();
                $("#serverError").fadeIn(500);
                //setInterval(hideServerErrorBlock, 6000);
            }
            ,
            statusCode: {
                500: function () {
                    /* $("#serverError").show(1000);
                    setInterval(hideServerErrorBlock, 4000);*/
                }
            }

        };

        if (!!this.customHeaders) {
            ajaxSettings.headers = this.customHeaders;
        }

        if (this.isJsonContentType) {
            //ajaxSettings.contentType = "application/json; charset=utf-8";
            ajaxSettings.dataType = "json";
            this.data = JSON.stringify(this.data);
        }

        if (!!request.axios) {
            return this._sendAxios(ajaxSettings);
        } else {
            return $.ajax(ajaxSettings);
        }
    };

    var hideServerErrorBlock = function () {
        $("#serverError").hide(1000);
    };

    Request.prototype.submit = function (form) {
        var request = this;
        if (request.showProccess)
            animation.showLoading();

        var ajaxSettings = {
            beforeSerialize: function () {
                //alert(tinyMCE.getContent());
                if (typeof tinyMCE != 'undefined') tinyMCE.get('elm1').save(); // 'content' is the id of your textarea
            },
            cache: false,
            dataType: 'json',
            success: function (result) {

                successCallback(result, request);
            },
            error: function (error) {
                animation.hideLoading();
                $("#serverError").show(1000);
                setInterval(hideServerErrorBlock, 6000);
            }
        };

        if (!!this.customHeaders) {
            ajaxSettings.headers = this.customHeaders;
        }

        form.ajaxSubmit(ajaxSettings);
        return false;
    };

    function successCallback(result, request) {
    	App.htmlHelper.canClosePopup = true;

    	request.addition.request = request;
        
        
        

        for (i in Request.ResponceCallbacks) {
            if (Request.ResponceCallbacks[i].id === request.id) {
                Request.ResponceCallbacks[i].function(result, request.addition);
            }
        }
        
        for (i in Request.AdditionsForRequest) {
            if (Request.AdditionsForRequest[i].id === request.id) {
                request.addition[Request.AdditionsForRequest[i].addition.key] = Request.AdditionsForRequest[i].addition.value;
            }
        }
        
        if (request.defaultMethod === true) {

            request.addAddition("request", request);
            Request.DefaultSuccessMethod(result, request.addition);
        }
        
        for (i in Request.RequestsSuccesses) {
            if (Request.RequestsSuccesses[i].id === request.id) {
                if (request.ok) {
                    Request.RequestsSuccesses[i].function(result, request.addition);
                }
                delete Request.RequestsSuccesses[i];
            }
        }

	    if (request.ok) {
		    for (i in request.successes) {
			    request.successes[i].function.apply(App.resp, [result, request.addition]);
		    }
	    } else {
	    	for (i in request.errorHandlers) {
	    		request.errorHandlers[i].function.apply(App.resp, [result, request.addition]);
	    	}
	    }


	    for (i in Request.RequestsPermanentSuccesses) {
            if (Request.RequestsPermanentSuccesses[i].id === request.id) {
                if (request.ok) {
                    Request.RequestsPermanentSuccesses[i].function(result, request.addition);
                }
            }
        }
        animation.hideLoading();
    }

    return Request;

}(App.Animation, $, App.resp);

;
App.CaptchaProvider = function ($, animation, url, requestProvider) {

    function GetCaptcha() {
        requestProvider.newRequest("Captcha-Get").post(url)
            .addSuccess(onGetCaptchaUrl)
            .offDefaultSuccess()
            .send();
    };

    function onGetCaptchaUrl(result) {
        //alert('dfgdf');
        $("img.captcha").attr('src', result);
        //alert('dfgdf2');
        $("img.captcha").parent().parent().show();
        //alert('dfgdf3');
        $("#reg_captcha").val("");
        animation.hideLoading();
    };

    return {
        GetCaptcha: GetCaptcha
    };
}($, App.Animation, App.Vars.urls.captcha, App.req);
;
class SubscribingObject {
    constructor() {
        this.subscribers = {};
    }
    addSubscriber(eventName, subscriber) {
        if (!this.subscribers[eventName]) {
            this.subscribers[eventName] = [];
        }
        this.subscribers[eventName].push(subscriber);
    }
    removeSubscriber(eventName, subscriber) {
        if (!this.subscribers[eventName]) {
            return;
        }
        const index = this.subscribers[eventName].indexOf(subscriber);
        if (index < 0) {
            return;
        }
        this.subscribers[eventName].splice(index, 1);
    }
    triggerEvent(eventName, data = {}) {
        const subscribers = this.subscribers[eventName];
        if (!subscribers)
            return;
        for (let i in subscribers) {
            subscribers[i](data);
        }
    }
}
//# sourceMappingURL=subscribing-object.js.map;
class LocalStorageDataItem {
    constructor(value) {
        this.value = value;
        this.isJSON = typeof value === 'object' ? true : false;
    }
}
class StorageService extends SubscribingObject {
    constructor() {
        super();
        this.currentVersion = '2';
        this.currentVersionKey = 'btm.tv.ls.v';
        // get current version as string
        const version = this.getLocalStorage().getItem(this.currentVersionKey);
        // clear ls if our version is older
        if (version !== this.currentVersion) {
            //this.clearStorage();
        }
        // save current version
        this.getLocalStorage().setItem(this.currentVersionKey, this.currentVersion);
        if (!!window) {
            window.addEventListener('storage', (e) => {
                this.triggerEvent(e.key);
            });
        }
    }
    save(key, data) {
        const localData = new LocalStorageDataItem(data);
        const value = JSON.stringify(localData);
        this.getLocalStorage().setItem(key, value);
    }
    load(key, defaultValue = null) {
        const localDataAsString = this.getLocalStorage().getItem(key);
        if (!localDataAsString) {
            return defaultValue;
        }
        const localData = JSON.parse(localDataAsString);
        return localData.value;
    }
    deleteItem(key) {
        //debugger;
        this.getLocalStorage().removeItem(key);
    }
    clearStorage() {
        this.getLocalStorage().clear();
    }
    getLocalStorage() {
        if (!!localStorage) {
            return localStorage;
        }
        // return null-object
        const message = 'localStorage doesn\'t support';
        return {
            clear: function () {
                console.warn(message);
            },
            removeItem: function () {
                console.warn(message);
            },
            setItem: function () {
                console.warn(message);
            },
            getItem: function () {
                console.warn(message);
            }
        };
    }
}
//# sourceMappingURL=storage.service.js.map;
App.LS = function () {


    function insertToLocalStorage(key, value) {
        if (window.localStorage) {
            try {
                window.localStorage.setItem(key, JSON.stringify(value));
            }
            catch (err) {
                window.localStorage.clear();
            }
        }
    }

    function getFromLocalStorage(key, defaultValue) {
        
        if (window.localStorage) {
            try {
                var string = window.localStorage.getItem(key);
	            if (string == null && !defaultValue) {
		            return null;
	            } else if (string == null && !!defaultValue){
					return defaultValue;
	            }
	            return JSON.parse(string);
            }
            catch (err) {
                return null;
            }
        }
        return null;
    }

    return {
        put: insertToLocalStorage,
        save: insertToLocalStorage,
        get: getFromLocalStorage
    };
}();
App.LS2 = new StorageService();
